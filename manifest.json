{
  "manifest_version": 2,
  "name": "Kitenator",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js","content.js"]
    }
  ],

  "web_accessible_resources":[
    "*.jpg"
  ]

}